    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.4"
  },
  "scripts": {
    "start": "concurrently \"cd landing-page && npm start\" \"cd dapp-frontend && npm start\"",
